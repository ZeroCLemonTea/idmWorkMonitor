        {
            "type": "group",
            "text": "基本属性",
            "desc": "提供设置组件能达到展示效果的基本属性设置信息",
            "children": []
        },
        {
            "type": "group",
            "text": "列表项样式",
            "desc": "点击？查看样式设置用法指南",
            "helpUrl": "",
            "children": [
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "宽",
                    "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                    "bindKey": "width",
                    "default": "100%"
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "高",
                    "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                    "bindKey": "height",
                    "default": "120px"
