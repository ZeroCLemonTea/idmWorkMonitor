                                "widthUnit": "",
                                "colors": {
                                    "hex8": ""
                                }
                            }
                        },
                        "radius": {
                            "leftTop": {
                                "radius": 50,
                                "radiusUnit": "%"
                            },
                            "rightTop": {
                                "radius": 50,
                                "radiusUnit": "%"
                            },
                            "leftBottom": {
                                "radius": 50,
                                "radiusUnit": "%"
                            },
                            "rightBottom": {
                                "radius": 50,
                                "radiusUnit": "%"
                            }
                        }
                    }
                },
                {
                    "type": "box",
                    "layoutType": "block",
                    "text": "图片边距",
                    "bindKey": "imageBox",
                    "default": {
                        "marginTopVal": "",
                        "marginRightVal": "",
                        "marginBottomVal": "10px",
                        "marginLeftVal": "",
                        "paddingTopVal": "",
                        "paddingRightVal": "",
                        "paddingBottomVal": "",
                        "paddingLeftVal": ""
                    }
                }
            ]
        },
        {
            "type": "group",
            "text": "状态组样式",
            "desc": "点击？查看样式设置用法指南",
            "helpUrl": "",
            "children": [
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "高",
                    "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                    "bindKey": "statusHeight",
                    "default": "82px"
                },
                {
                    "type": "border",
                    "layoutType": "block",
                    "text": "边框",
                    "bindKey": "statusBorder",
                    "default": {
                        "border": {
                            "top": {
                                "style": "solid",
                                "width": 1,
                                "widthUnit": "px",
                                "colors": {
                                    "hex8": ""
                                }
                            },
                            "right": {
                                "style": "solid",
                                "width": 1,
                                "widthUnit": "px",
                                "colors": {
                                    "hex8": ""
                                }
                            },
                            "bottom": {
                                "style": "solid",
                                "width": 1,
                                "widthUnit": "px",
                                "colors": {
                                    "hex8": ""
                                }
                            },
                            "left": {
                                "style": "solid",
                                "width": 1,
                                "widthUnit": "px",
                                "colors": {
                                    "hex8": ""
                                }
                            }
                        },
                        "radius": {
                            "leftTop": {
                                "radius": 6,
