                },
                {
                    "type": "border",
                    "layoutType": "block",
                    "text": "边框",
                    "bindKey": "border",
                    "default": {
                        "border": {
                            "top": {
                                "style": "",
                                "width": 0,
                                "widthUnit": "x",
                                "colors": {
                                    "hex8": ""
                                }
                            },
                            "right": {
                                "style": "",
                                "width": 0,
                                "widthUnit": "x",
                                "colors": {
                                    "hex8": ""
                                }
                            },
                            "bottom": {
                                "style": "dashed",
