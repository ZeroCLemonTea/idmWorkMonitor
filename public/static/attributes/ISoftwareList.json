                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "软件文字",
                    "bindKey": "softwareFont",
                    "default": {
                        "fontFamily": "PingFangSC-Medium",
                        "fontColors": {
                            "hex": "#333333",
                            "hex8": "#333333FF"
                        },
                        "fontWeight": "400",
                        "fontStyle": "",
                        "fontSize": 16,
                        "fontSizeUnit": "px",
                        "fontTextAlign": "center",
                        "fontDecoration": "",
                        "fontLetterSpacing": 0,
                        "fontLetterSpacingUnit": "px"
                    }
                },
                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "数量文字",
                    "bindKey": "numberFont",
                    "default": {
                        "fontFamily": "PingFangSC-Medium",
                        "fontColors": {
                            "hex": "#333333",
                            "hex8": "#333333FF"
                        },
                        "fontWeight": "600",
                        "fontStyle": "",
                        "fontSize": 30,
                        "fontSizeUnit": "px",
                        "fontTextAlign": "",
                        "fontDecoration": "",
                        "fontLetterSpacing": 0,
                        "fontLetterSpacingUnit": "px"
