                                "radiusUnit": "px"
                            },
                            "leftBottom": {
                                "radius": 6,
                                "radiusUnit": "px"
                            },
                            "rightBottom": {
                                "radius": 6,
                                "radiusUnit": "px"
                            }
                        }
                    }
                },
                {
                    "type": "box",
                    "layoutType": "block",
                    "text": "内外边距",
                    "bindKey": "statusBox",
                    "default": {
                        "marginTopVal": "",
                        "marginRightVal": "30px",
                        "marginBottomVal": "",
                        "marginLeftVal": "",
                        "paddingTopVal": "",
                        "paddingRightVal": "",
                        "paddingBottomVal": "",
                        "paddingLeftVal": ""
