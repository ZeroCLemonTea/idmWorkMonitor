                    }
                },
                {
                    "type": "border",
                    "layoutType": "block",
                    "text": "左侧边框",
                    "bindKey": "leftBorder"
                },
                {
                    "type": "box",
                    "layoutType": "block",
                    "text": "左侧边距",
                    "bindKey": "leftBox",
                    "default": {
                        "marginTopVal": "",
                        "marginRightVal": "",
                        "marginBottomVal": "",
                        "marginLeftVal": "",
                        "paddingTopVal": "",
                        "paddingRightVal": "",
                        "paddingBottomVal": "",
                        "paddingLeftVal": ""
