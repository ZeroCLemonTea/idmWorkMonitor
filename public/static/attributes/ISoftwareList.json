                {
                    "type": "box",
                    "layoutType": "block",
                    "text": "图标边距",
                    "bindKey": "iconBox",
                    "default": {
                        "marginTopVal": "",
                        "marginRightVal": "15%",
                        "marginBottomVal": "",
                        "marginLeftVal": "",
                        "paddingTopVal": "",
                        "paddingRightVal": "",
                        "paddingBottomVal": "",
                        "paddingLeftVal": ""
                    }
                },
                {
                    "type": "box",
                    "layoutType": "block",
                    "text": "数字边距",
                    "bindKey": "numberBox",
                    "default": {
                        "marginTopVal": "",
                        "marginRightVal": "",
                        "marginBottomVal": "",
                        "marginLeftVal": "6px",
                        "paddingTopVal": "",
                        "paddingRightVal": "",
                        "paddingBottomVal": "",
                        "paddingLeftVal": ""
                    }
                }
            ]
        },
        {
            "type": "table",
            "text": "软件图片配置",
            "bindKey": "imageList",
            "tabText": "软件[index]",
            "default": [
                {
                    "softwareName": "数据库",
                    "image": "defaultImage"
                },
                {
                    "softwareName": "缓存",
                    "image": "defaultImage"
                },
                {
                    "softwareName": "Web中间件",
                    "image": "defaultImage"
                },
                {
                    "softwareName": "Elasticsearch",
                    "image": "defaultImage"
                },
                {
                    "softwareName": "消息中间件",
                    "image": "defaultImage"
                }
            ],
            "children": [
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "软件名称",
                    "bindKey": "softwareName"
                },
                {
                    "type": "uploadImage",
                    "layoutType": "inline",
                    "text": "显示图片",
                    "bindKey": "image"
                }
            ]
        },
        {
            "type": "table",
            "text": "状态组配置",
            "bindKey": "statusList",
            "tabText": "状态[index]",
            "default": [
                {
                    "status": "正常",
                    "defaultIcon": "duihao",
                    "numberField": "normalNumber",
                    "color": {
                        "hex8": "#44D7B6FF"
                    },
                    "iconSize": "34px"
                },
                {
                    "status": "异常",
                    "defaultIcon": "gantanhao",
                    "numberField": "warningNumber",
                    "color": {
                        "hex8": "#F7B500FF"
                    },
                    "iconSize": "34px"
                },
                {
                    "status": "关闭",
                    "defaultIcon": "chahao",
                    "numberField": "closeNumber",
                    "color": {
                        "hex8": "#CDCDCDFF"
                    },
                    "iconSize": "34px"
                }
            ],
            "children": [
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "状态文本",
                    "bindKey": "status"
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "数量字段",
                    "bindKey": "numberField"
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "默认icon",
                    "bindKey": "defaultIcon",
                    "display": "@[false]"
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "icon尺寸",
                    "bindKey": "iconSize"
                },
                {
                    "type": "colorPicker",
                    "layoutType": "inline",
                    "text": "状态颜色",
                    "bindKey": "color"
                },
                {
                    "type": "iconSelect",
                    "layoutType": "inline",
                    "text": "显示图标",
                    "bindKey": "icon"
                }
            ]
        },
        {
            "type": "group",
            "text": "高级",
            "desc": "点击？查看样式设置用法指南",
            "helpUrl": "",
            "children": [
                {
                    "type": "select",
                    "layoutType": "inline",
                    "text": "数据模式",
                    "bindKey": "dataType",
                    "default": "dataSource",
                    "dictionary": [
                        {
                            "label": "数据源",
                            "value": "dataSource"
                        },
                        {
                            "label": "页面统一接口",
                            "value": "pageCommonInterface"
                        }
                    ]
                },
                {
                    "type": "dataSourceSelect",
                    "text": "数据源",
                    "layoutType": "inline",
                    "display": "@[dataType == 'dataSource']",
                    "bindKey": "dataSource"
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "结果集名",
                    "bindKey": "dataName",
                    "desc": "页面接口设定的结果集名称，位置为：页面设置》高级设置》页面接口",
                    "default": "",
                    "placeholder": "示例：resultData",
                    "display": "@[dataType=='pageCommonInterface']"
                },
                {
                    "type": "textarea",
                    "layoutType": "inline",
                    "text": "显示字段",
                    "bindKey": "dataFiled",
                    "desc": "根据接口返回数据格式指定结果集的字段，比如结果集名为resultData（自定义接口忽略）且它的值为{data:{filedName:[{\"text\":\"\",\"value\":\"\",\"check\":true}]}}，则这里应该填写data.filedName",
                    "default": "",
                    "placeholder": "示例：data.filedName 或者 [0].data[0].filedName",
                    "display": "@[dataType=='pageCommonInterface']"
                }
            ]
        }
    ]
}
